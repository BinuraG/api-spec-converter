// http://eslint.org/docs/rules
{
  "extends": "stoplight",

  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": 0,
    "prefer-template": 0,
    "func-names": 0,
    "eqeqeq": 0,
    "prefer-arrow-callback": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "no-param-reassign": 0,
    "no-empty":0,
    "block-scoped-var":0,
    "no-prototype-builtins":0,
    "spaced-comment":0,
    "no-tabs": 0,
    "keyword-spacing":0,
    "no-continue":0,
    "key-spacing":0,
    "no-trailing-spaces":0,
    "no-useless-escape":0,
    "no-multi-spaces":0,
    "no-mixed-spaces-and-tabs":0
  }
}
