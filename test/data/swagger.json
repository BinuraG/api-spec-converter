{
  "swagger": "2.0",
  "schemes": [
    "http",
    "https",
    "ws"
  ],
  "basePath": "/api",
  "host": "localhost:3000",
  "info": {
    "version": "1.0.0",
    "title": "Swagger petstore",
    "description": "A sample API"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/pets/{petName}": {
      "parameters": [
        {
          "name": "petName",
          "in": "path",
          "description": "name for the pet",
          "required": true,
          "type": "string"
        }
      ],
      "get": {
        "description": "Returns a pet by name",
        "operationId": "findPetByName",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "400": {
            "description": "Returns the pet data",
            "schema": {
              "$ref": "#/definitions/pet"
            }
          }
        },
        "summary": ""
      },
      "delete": {
        "description": "Deletes a single pet based on the name supplied",
        "operationId": "deletePet",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "400": {
            "description": "Returns the pet that was deleted",
            "schema": {
              "$ref": "#/definitions/pet"
            }
          }
        },
        "summary": ""
      },
      "patch": {
        "description": "Updates a pet by name",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "$ref": "#/definitions/pet"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Returns the updated pet data",
            "schema": {
              "$ref": "#/definitions/pet"
            }
          }
        },
        "summary": ""
      }
    },
    "/pets/{petName}/photos": {
      "parameters": [
        {
          "name": "petName",
          "in": "path",
          "description": "name for the pet",
          "required": true,
          "type": "string"
        }
      ],
      "post": {
        "description": "Upload a new pet photo",
        "operationId": "addPetPhoto",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "properties": {
                "id": {
                  "description": "The photo ID (generated automatically)",
                  "type": "string"
                },
                "label": {
                  "description": "A label for the photo",
                  "type": "string"
                },
                "description": {
                  "description": "An optional description of the photo",
                  "type": "string"
                },
                "photo": {
                  "description": "The pet photo",
                  "type": "string"
                }
              },
              "required": [
                "photo"
              ]
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Returns the photo information",
            "schema": {
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32",
                  "description": "The auto-generated photo ID"
                },
                "label": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "photo": {
                  "description": "Information about the photo (size, file name, etc.)"
                }
              }
            }
          }
        },
        "summary": ""
      }
    }
  },
  "definitions": {
    "pet": {
      "required": [
        "name",
        "type"
      ],
      "properties": {
        "name": {
          "type": "string",
          "minLength": 4,
          "pattern": "^[a-zA-Z0-9- ]+$"
        },
        "age": {
          "type": "integer"
        },
        "dob": {
          "type": "string",
          "format": "date"
        },
        "type": {
          "type": "string",
          "enum": [
            "cat",
            "dog",
            "bird"
          ]
        },
        "address": {
          "$ref": "#/definitions/address"
        },
        "vet": {
          "$ref": "#/definitions/veterinarian"
        },
        "tags": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "veterinarian": {
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "address": {
          "$ref": "#/definitions/address"
        }
      }
    },
    "address": {
      "properties": {
        "street": {
          "type": "string",
          "minLength": 1
        },
        "city": {
          "type": "string",
          "minLength": 1
        },
        "state": {
          "type": "string",
          "minLength": 2,
          "maxLength": 2,
          "pattern": "^[A-Z]+$"
        },
        "zipcode": {
          "type": "integer",
          "minimum": 10000,
          "maximum": 99999
        }
      }
    }
  }
}
